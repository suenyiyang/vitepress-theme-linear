{
  "name": "vitepress-theme-linear",
  "version": "0.2.6",
  "packageManager": "pnpm@6.32.6",
  "description": "Linear theme for vitepress.",
  "keywords": [],
  "license": "MIT",
  "author": "syy11cn",
  "main": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts"
    },
    "./config": {
      "import": "./dist/config.mjs",
      "require": "./dist/config.js",
      "types": "./dist/config.d.ts"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "dev": "vitepress dev docs",
    "build:docs": "vitepress build docs",
    "build": "rimraf dist && tsup src/config.ts --dts --format cjs,esm --minify --out-dir dist",
    "lint": "eslint ./src/**/*.{ts,vue} --fix",
    "prepublishOnly": "npm run build",
    "serve": "vitepress serve docs"
  },
  "peerDependencies": {
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "@vueuse/head": "^1.1.26",
    "@waline/client": "^2.15.5",
    "fs-extra": "^10.1.0",
    "globby": "11.0.4",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.21.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.27",
    "eslint": "^8.14.0",
    "rimraf": "^5.0.1",
    "tsup": "^6.7.0",
    "typescript": "^4.6.4",
    "vitepress": "1.0.0-alpha.76",
    "vue": "^3.2.33"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
